{
  "name": "@keepkey/keepkey-bridge",
  "version": "0.0.2",
  "description": "A KeepKey support application",
  "main": "dist/main.js",
  "author": "highlander <bithighlander@gmail.com>",
  "scripts": {
    "npm": "npm i",
    "npm-upgrade": "npm update",
    "test": "echo \"No test specified\" && exit 0",
    "clean": "rm -Rf node_modules dist api",
    "refresh": "rm -Rf node_modules dist api && npm i",
    "api-dev": "nodemon --watch 'src-api/controllers/*.ts' --exec npm run build:all && node dist/main -W",
    "build:routes": "mkdir -p src-api/routes && tsoa routes",
    "build:swagger": "mkdir -p api && mkdir -p api/dist && tsoa swagger",
    "build:routes-local": "mkdir -p src-api/routes && tsoa routes --configuration tsoaLocal.json",
    "build:swagger-local": "mkdir -p api && mkdir -p api/dist && tsoa swagger --configuration tsoaLocal.json",
    "build:ts": "tsc -p src-api",
    "build": "npm run build:routes-local && npm run build:swagger-local && npm run build:ts",
    "dev": "npm run build:routes-local && npm run build:swagger-local && npm run build:ts && node __tests__/test-module.js",
    "start": "node dist/main.js",
    "docker:build": "docker build -t pioneer/pioneer-server:latest .",
    "docker:tag": "docker tag pioneer/pioneer-server:latest registry.digitalocean.com/pioneer/pioneer/pioneer-server:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/pioneer/pioneer-server:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "lint": "tslint -c tslint.json 'src-api/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src-api/**/*.ts'"
  },
  "dependencies": {
    "@keepkey/keepkey-hardware": "^0.0.1",
    "@pioneer-platform/loggerdog": "^8.1.17",
    "@quasar/extras": "^1.10.4",
    "@types/node": "^12.20.11",
    "autoprefixer": "^9.8.6",
    "axios": "^0.18.1",
    "bnc-notify": "^1.6.1",
    "bnc-onboard": "^1.26.0",
    "core-js": "^3.12.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ethers": "^5.1.4",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "method-override": "^3.0.0",
    "numbro": "^2.3.2",
    "object-query-string": "^1.1.2",
    "quasar": "^1.15.12",
    "query-string": "^6.14.0",
    "randomstring": "^1.1.5",
    "rate-limiter-flexible": "^2.2.1",
    "redredisearch": "0.0.1",
    "short-uuid": "^3.1.1",
    "socket.io": "^2.4.1",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.7.0",
    "typescript": "^3.9.9",
    "url-parse": "^1.5.1",
    "vue-i18n": "^8.24.4",
    "wait-promise": "^0.4.1"
  },
  "devDependencies": {
    "@quasar/app": "^2.2.6",
    "tslint": "^5.20.0"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "gitHead": "4921814f1c6d33a4eca9015ba544d80e1cdd0f27"
}
